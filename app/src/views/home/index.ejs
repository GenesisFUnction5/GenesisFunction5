    <script>
        let myPageOnOff;
        myPageBtn = document.getElementById("myPageBtn")
        document.getElementById("myPageBox").style.display = "none";
        
        myPageOnOff = function(e) {
            myPageDisplay = document.getElementById("myPageBox").style.display;

            if(myPageDisplay == "none") { //myPage 체크박스가 안되있으면 (꺼져있으면) 클릭할 때
                if(e.target == myPageBtn) { // target id가 myPageBtn이면
                    document.getElementById("myPageBox").style.display = "block";
                    document.getElementById("quickMenu").style.display = "none";
                    return;
                }
                else { // target이 버튼이 아니라면
                    return;
                }
            }
            else {//myPage 체크박스가 체크 되있으면 (마이페이지 켜져있으면)
                if(e.target.id != "myPageBtn" && e.target.id != "closeBtn") {  //click 타겟의 id가 myPageBtn과 closeBtn이 아니면 
                    let tgE1 = e.target;
                    let myPageBox = tgE1.closest("#myPageBox"); //타겟의 조상중 id가 myPageBox 있는지 찾음
                    if(!myPageBox) { //타겟의 조상중 myPageBox이 없으면 시행
                        document.getElementById("myPageBox").style.display = "none";
                        document.getElementById("quickMenu").style.display = "block";
                    } 
                    else { //조상중 myPageBox 있으면(myPage 내부 클릭)
                        return;
                    }
                }
                else { //click target id가 myPageBtn이거나 closeBtn이면
                    document.getElementById("myPageBox").style.display = "none";
                    document.getElementById("quickMenu").style.display = "block";
                }
            }
        }

        document.addEventListener("click", myPageOnOff);

